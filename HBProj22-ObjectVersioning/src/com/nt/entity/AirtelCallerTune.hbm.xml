<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 <hibernate-mapping>
    <class name="com.nt.entity.AirtelCallerTune" table="AIRTEL_CALLERTUNE">

      <id  name="custId" column="CUSTID" type="int">
        <generator class="increment"/>
      </id>
      <version name="updationsCount" column="UPDATIONSCOUNT" type="int" />
      <property name="custName" column="CUSTNAME" length="20" type="string"/>
      <property name="mobileNo" column="mobileNo" length="10"/>
      <property name="callerTune" column="CALLERTUNE" length="20"/>
    </class>
 
 
 </hibernate-mapping>       